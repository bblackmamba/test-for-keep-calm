{
  "name": "test-for-keep-calm",
  "description": "Test task for the company Keep Calm",
  "author": "Bogdan Shevchenko",
  "packageManager": "yarn@3.4.1",
  "scripts": {
    "test": "jest"
  },
  "devDependencies": {
